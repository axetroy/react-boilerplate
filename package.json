{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "6.1.2",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.1.9",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.3",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/react-router-dom": "5.1.7",
    "@types/react-transition-group": "4.4.1",
    "antd": "4.16.3",
    "axios": "0.21.1",
    "craco-alias": "3.0.1",
    "craco-less": "1.17.1",
    "husky": "5.2.0",
    "prettier": "2.3.1",
    "pretty-quick": "3.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.4",
    "react-transition-group": "4.4.2",
    "typescript": "4.3.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "format": "prettier --write \"**/*.*(js|jsx|ts|tsx|md|css|less|sass|scss|yml|yaml|json)\"",
    "format-check": "prettier --check \"**/*.*(js|jsx|ts|tsx|md|css|less|sass|scss|yml|yaml|json)\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern \"**/*.*(js|jsx|ts|tsx|md|css|less|sass|scss|yml|yaml|json)\""
    }
  }
}
